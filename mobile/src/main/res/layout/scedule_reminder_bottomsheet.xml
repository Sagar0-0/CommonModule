<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="5 mins befor the set time" />

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="10 mins befor the set time" />

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="15 mins befor the set time" />

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="20 mins befor the set time" />

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="25 mins befor the set time" />

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="30 mins befor the set time" />

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="40 mins befor the set time" />

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="50 mins befor the set time" />

        <TextView
            android:padding="@dimen/dimen_5"
            android:textSize="20sp"
            android:id="@+id/textView18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="60 mins befor the set time" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>